{
    "name": "xabierlegasa/design-patterns",
    "minimum-stability": "dev",
    "description": "Patterns examples in PHP from O'reilly's Head First Design Patterns",
    "keywords": ["design-patterns", "php"],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "DesignPatterns\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "DesignPatterns\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}